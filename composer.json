{
    "name": "sinergi/core",
    "require": {
        "sinergi/config": "0.4.*",
        "sinergi/dictionary": "0.2.*",
        "sinergi/gearman": "0.5.*",
        "sinergi/registry": "0.1.*",
        "klein/klein": "dev-master",
        "twig/twig": "1.15.*",
        "symfony/console": "2.5.*",
        "doctrine/orm": "2.*",
        "psr/log": "~1"
    },
    "require-dev": {
        "facebook/xhprof": "dev-master",
        "phpunit/phpunit": "~4",
        "phpunit/dbunit": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Core\\": "src/Core"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Core\\Tests\\": ["tests/Core/Tests", "tests/Core/Tests/_includes"]
        }
    }
}
